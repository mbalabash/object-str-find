  "description": "Find substring or match regexp in the object and his inner objects.",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint ./src --cache --fix",
    "test": "ava"
  },
  "dependencies": {
    "ava": "^1.2.0"
  },
  "devDependencies": {
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
